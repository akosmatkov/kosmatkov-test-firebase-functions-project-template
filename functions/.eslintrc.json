{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "quotes": ["error", "double"],
    "require-jsdoc": "off",
    "valid-jsdoc": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "ignorePatterns": ["lib/*"],
}
